<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/setting_bgcolor"
    android:orientation="vertical"
    >

    <RelativeLayout
        android:id="@+id/sys_map_layout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="6dip"
        android:paddingLeft="11dip"
        android:paddingRight="9dip"
        android:paddingTop="6dip" >

        <RelativeLayout
            android:id="@+id/line1"
            android:layout_width="95dip"
            android:layout_height="fill_parent" >

            <TextView

                android:id="@+id/TextViewValue1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:gravity="left"
                android:text="@string/sys_map_value1"
                android:textSize="@dimen/app_content_txt_size" />

            <TextView
                android:id="@+id/textView0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginTop="8dip"
                android:gravity="left"
                android:text="@string/pref_lt"
                android:textSize="@dimen/app_content_txt_size" />
        </RelativeLayout>

        <com.walktour.framework.ui.BasicSpinner
            android:id="@+id/SpinnerValue1"
            android:layout_width="102dip"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:background="@drawable/drop"
            android:drawSelectorOnTop="true" />

        <EditText
            android:id="@+id/EditTextValue1"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_centerVertical="true"
            android:layout_marginLeft="9dip"
            android:layout_marginRight="9dip"
            android:layout_toLeftOf="@id/SpinnerValue1"
            android:layout_toRightOf="@id/line1"
            android:background="@drawable/edt_drop"
            android:editable="false"
            android:focusable="false"
            android:maxLength="10"
            android:paddingLeft="4dip"
            android:singleLine="true"
            android:textColor="@android:color/white"
            android:textSize="@dimen/app_content_txt_size" />
    </RelativeLayout>

    <View
        android:id="@+id/line_layout_1"
        style="@style/task_listdivider" />

